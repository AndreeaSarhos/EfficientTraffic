<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andreea.traficeficient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.andreea.traficeficient.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.andreea.traficeficient.Aleatoriu" />
        <activity android:name="com.example.andreea.traficeficient.Numerar" />
        <activity
            android:name="com.example.andreea.traficeficient.Meniu"
            android:label="TraficEficient"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>